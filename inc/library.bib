	Pages = {4244--4252},
	Title = {{A Single Intramuscular Injection of Recombinant Plasmid DNA Induces Protective Immunity and Prevents Japanese Encephalitis in Mice}},
	Url = {http://jvi.asm.org/cgi/content/abstract/74/9/4244},
	Volume = {74},
