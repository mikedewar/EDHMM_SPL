	Journal = {Computational biology and chemistry},
	Keywords = {Algorithms,Base Sequence,Computational Biology,Computational Biology: methods,Genome, Viral,RNA Splice Sites,RNA Viruses,RNA Viruses: genetics,Sequence Analysis, RNA},
	Month = apr,
	Number = {2},
