	Keywords = {Adult,Antigens,CCR5,CCR5: metabolism,CD4,CD4: metabolism,CXCR4,CXCR4: metabolism,Computational Biology,Computational Biology: methods,Diagnostic,Female,Genotype,HIV Envelope Protein gp120,HIV Envelope Protein gp120: genetics,HIV Envelope Protein gp120: metabolism,HIV Infections,HIV Infections: virology,HIV-1,HIV-1: classification,HIV-1: genetics,HIV-1: metabolism,HIV-1: pathogenicity,Humans,Male,Middle Aged,Peptide Fragments,Peptide Fragments: genetics,Peptide Fragments: metabolism,Phenotype,Reagent Kits,Receptors,Sensitivity and Specificity},
	Month = mar,
