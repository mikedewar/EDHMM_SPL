@article{Azad2007,
	Abstract = {Most parametric methods for detecting foreign genes in bacterial genomes use a scoring function that measures the atypicality of a gene with respect to the bulk of the genome. Genes whose features are sufficiently atypical-lying beyond a threshold value-are deemed foreign. Yet these methods fail when the range of features of donor genomes overlaps with that of the recipient genome, leading to misclassification of foreign and native genes; existing parametric methods choose threshold parameters to balance these error rates. To circumvent this problem, we have developed a two-pronged approach to minimize the misclassification of genes. First, beyond classifying genes as merely atypical, a gene clustering method based on Jensen-Shannon entropic divergence identifies classes of foreign genes that are also similar to each other. Second, genome position is used to reassign genes among classes whose composition features overlap. This process minimizes the misclassification of either native or foreign genes that are weakly atypical. The performance of this approach was assessed using artificial chimeric genomes and then applied to the well-characterized Escherichia coli K12 genome. Not only were foreign genes identified with a high degree of accuracy, but genes originating from the same donor organism were effectively grouped.},
	Author = {Azad, Rajeev K and Lawrence, Jeffrey G},
	Doi = {10.1093/nar/gkm204},
	File = {::},
	Issn = {1362-4962},
	Journal = {Nucleic acids research},
	Keywords = {Cluster Analysis,Escherichia coli K12,Escherichia coli K12: genetics,Gene Transfer, Horizontal,Genome, Bacterial,Genomics,Genomics: methods,Phylogeny},
	Month = jan,
	Number = {14},
	Pages = {4629--39},
	Pmid = {17591616},
	Title = {{Detecting laterally transferred genes: use of entropic clustering methods and genome position.}},
	Url = {http://nar.oxfordjournals.org/cgi/content/abstract/35/14/4629},
	Volume = {35},
